										reviewsUIManager.setDisplayMode(HZ.reviews.ProductReviewsUIManager.DISPLAY_MODE_NEWPAGE);
										var handler = $.proxy(reviewsUIManager.onExpandClick, reviewsUIManager);
					$("#noReviewsCta, .review-actions > .edit").click( handler );
					$(document).ready(function(){
						reviewsUIManager.setLoadMoreElement($("#moreReviews"));
											});
				})());
			</script>
			


