		window.hzmr.allResources = ["jquery-1.11.1","hz","hzConfig","hzI18n","hzLogger","hzMain","eventBus","main","bmblock","errSt","jquery-migrate-1.2.1","jqUtils","core","ui","responsiveReload","autoSuggest","standardHeaderV6","NewGalleryRequest","CartActionsRequest","SetVisitorPropertyRequest","LightboxGetNextSpacesRequest","LightboxGetSpaceDataRequest","AddImageTagRequest","ProductLookupRequest","GetManufacturerColorsRequest","SaveColorPaletteRequest","ReportProductAjaxRequest","SendEmailRequest","CurrentSessionUserInfoRequest","GetProfessionalProjectsRequest","GetProfessionalProjectPhotosRequest","GetHousesByGalleryRequest","BuzzAjaxRequest","GetMinimalSpaceDataRequest","HoverCardRequest","FollowRequest","CommentManagerAjaxRequest","FeedAjaxRequest","ContentFlagAjaxRequest","LikeRequest","ClippingRequest","ReviewAjaxRequest","SearchSuggestionRequest","VerificationEmailRequest","NoOpRequest","BuzzRequest","GetEmbedSpaceCodeRequest","GetQuestionsForSpaceRequest","UserPreferencesRequest","TrustPilotAjaxRequest","WelcomeToHouzzAjaxRequest","AuthorizeRequest","AttributesAjaxRequest","TsdbLoggerAjaxRequest","JsErrorLoggerAjaxRequest","SearchKeywordRequest","GetSpaceAjaxRequest","spaceActions","socialSdk","siteAuth","filterRenderer","hzGtm","standardFooter","scrollToTop","cartActions","signupDialog","addToIdeabookDialog","saveBasicProfileDialog","history","jquery.easing.1.3","jquery.visible","md5","lightboxStaticConfig","lightboxContext","lightboxUtils","lightbox2.2","productVariation","productListingInfoRenderer","availabilityNotification","qtySelector","reportProduct","lightboxColorPickerUtils","lightboxColorPicker2","lightboxImageTags2","contactDialog","lightbox2.2AdPlugins","lightbox2.2Plugins","jQ-AjaxFileUpload","summernote","jquery.htmlClean","richTextEditor","questionRichTextEditor","commentRichTextEditor","newPostSignup","hoverCard","followButton","fontSmoothing","jquery-ui-1.10.3.custom.widget-only","flagContent","feedComments","jquery.form.min","spacePreview","zoomable","like","clipping","returnPolicy","reviewProductUI","multipleImagesViewer","rateStarsLabels","jquery.rateStars","dropzone","viewSpace","productZoom","imageTag2","jquery.scrollTo"];
		for (var i=0; i<window.hzmr.length; i++) {
			var filename = window.hzmr[i];
			var v = filename.split(':')[1];
			if (v != window.hzmr.version) {
				HZ.utils.Logger.sendError({msg:"Page version "+window.hzmr.version+" doesn't match file version "+filename, file:null, line:null, url:window.location.href});
				break;
			}
		}
		var missingFiles = [];
		var loadedResources = {};
		for (var i=0; i<hzmr.length; i++) { loadedResources[hzmr[i].match(/(.*):\d*/)[1]] = true; }
		for (var i=0; i<hzmr.allResources.length; i++) {
			key = hzmr.allResources[i];
			if (!loadedResources[key]) {
				missingFiles.push (key);
			}
		}
		if (missingFiles.length > 0) {
			var msg = "JS resource failed to load: " + missingFiles[0] + ".";
			if (missingFiles.length > 1) {
				msg += " Missing out on: " + missingFiles.slice(1).join(",") + ".";
			}
			HZ.utils.Logger.sendError({msg:msg, file:null, line:null, url:window.location.href});
		}
	}
})();
</script>
