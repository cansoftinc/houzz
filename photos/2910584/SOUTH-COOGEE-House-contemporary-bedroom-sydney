								$('#mpAddToCartBtn').click(function () {
						var qty = $('#qtySelector').val(),
							vlId = $(this).attr('vlId'),
							houseId = $(this).attr('hid');
						if (qty) {
							HZ.cart.Actions.addItem(houseId, vlId, qty);
						}
					});
					$('.buy-button[data-listing-id]').click(function () {
						var lid = $(this).data("listingId");
						var sid = $(this).data("spaceId");
						HZ.cart.Actions.addItem(sid, lid, 1);
					});
