			HZ.spaceActions.AddToIdeabookDialog.setWishListIdeabookId (null);

			HZ.spaceActions.AddToIdeabookDialog.setDefaultGalleryTitle("");
			HZ.spaceActions.AddToIdeabookDialog.setShowIdeabookConfirm(true);	
			HZ.spaceActions.AddToIdeabookDialog.setWelcomeScreen(false);
			HZ.spaceActions.AddToIdeabookDialog.setImageId(null);
		    HZ.spaceActions.AddToIdeabookDialog.setUserName(null);
		    HZ.spaceActions.AddToIdeabookDialog.setTrustPilotReview(false);
			HZ.spaceActions.AddToIdeabookDialog.setOnSuccess(function(){showSaveBasicProfileDialog("Saved! Now introduce yourself")});
			HZ.spaceActions.QuestionDialog.JSON_KEY_SPACE_ID = "spaceId";
			HZ.spaceActions.QuestionDialog.JSON_KEY_QUESTIONS = "questions";
			HZ.spaceActions.QuestionDialog.JSON_KEY_DAILY_QUESTIONS_QUOTA_EXCEEDED = "quotaExceeded";
		    HZ.spaceActions.QuestionDialog.setQuestionUpsellLink("http:\/\/www.houzz.com\/discussions\/design-dilemma");

