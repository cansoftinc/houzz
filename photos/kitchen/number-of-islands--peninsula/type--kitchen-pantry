		<script type="text/javascript">
			var _h_url_leaderboardBanner = [];
			function _h_url_dek_leaderboardBanner(id) {
				var r = id.match(/\d+/);
				if (r >= 0)
				{
					var data = _h_url_leaderboardBanner[r];
					return data[0] + "=" + data[2] + data[1];
				}
			}

			function _h_url_click_leaderboardBanner(x) {
				var u = _h_url_dek_leaderboardBanner(x.id);
				if (u) {
					x.href = u;
				}
			}
			
			function _h_chk_url_used_leaderboardBanner(id) {
				var r = id.match(/\d+/);
				if (r >= 0)
				{
					var data = _h_url_leaderboardBanner[r];
					return (data[3] != 0);
				}
				return true;
			}
			
			function _h_set_url_used_leaderboardBanner(id) {
				var r = id.match(/\d+/);
				if (r >= 0)
				{
					var data = _h_url_leaderboardBanner[r];
					data[3] = 1;
					_h_url_leaderboardBanner[r] = data;
				}
			}
			
		</script>
