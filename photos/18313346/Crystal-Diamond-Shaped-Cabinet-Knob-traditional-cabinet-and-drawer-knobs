			$(document).ready(function(){
				if (HZ.QtySelector) {
					var qtySelector = new HZ.QtySelector();
					qtySelector.init(document.getElementById('qtySelector'));
				}

				$(".productSpec .hzAttributes .p65d").click(function(e){
					HZ.ui.yamdi.Common.alert(_hgt("WARNING"), _hgt("This product contains a chemical known to the State of California to cause cancer, birth defects, or other reproductive harm."));
				});
				HZ.module.AvailabilityNotification.setMountNode($("#availabilityNotificationBtn"));
								HZ.module.AvailabilityNotification.setABTestValue(false);
				HZ.module.AvailabilityNotification.updateView(18313346, "viewSpace", true, 75, "hzBtn addToCart");
									HZ.listingInfo.Renderer.setMountNode($("#hzProductInfo .seller-shipping-info").get(0));
						HZ.listingInfo.Renderer.updateView(18313346,"7794022", false);
						// Update quantity to max available
						if($('#qtySelector').length){
							HZ.cart.Actions.subscribeCartUpdate("ViewSpace", function(evt, response) {
								if (response.operation === 'add' && response.message && response.message.q) {
									$('#qtySelector').val(response.message.q);
								}
							});
						}
		        			});
            (function(){
                var fixScroll = true;
                window.onload = function(){
                    setTimeout(function(){
                        fixScroll && scrollTo(0,-1);
                    },0);
                };
                $(document).ready(function(){
                	$(window).on("mousedown wheel touchstart", function(){
                		fixScroll = false;
                	});
                });
            })();
		</script>
