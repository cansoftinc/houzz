	if (sendFeedback) {
		HZ.utils.Logger.sendEventLogLightweight("fontSmoothing", {"hasSmoothing":hasSmoothing});
	}
	$("head").append("<link>");
	$("head").children(":last").attr({
		rel:  "stylesheet",
		type: "text/css",
		href: HZ.utils.Links.getCssUrl("fontSmoothing") + "&ts=" + new Date().getTime()
	});
	// append class to body tag.
	TypeHelpers.insertClasses();
});		
</script>
		


