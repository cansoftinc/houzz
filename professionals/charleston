						var currentLocation ="";
						var separator = "/";
						if(currentLocation!="") {
							currentLocation = currentLocation.replace(/[\s+,]/g,"-");
							currentLocation = separator+"c"+separator+encodeURI(currentLocation);
						}
						var url = document.URL;
						if(url.substr(url.length-1,url.length) === "#") {
							document.body.style.visibility = "hidden";
							document.location = url.replace("#",currentLocation);
						} else {
							setCookie("loc","Charleston Metro",1000,"/");
