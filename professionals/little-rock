				var r = id.match(/\d+/);
				if (r >= 0)
				{
					var data = _h_url_paid_pro[r];
					return data[0] + "=" + data[2] + data[1];
				}
			}

			function _h_url_click_paid_pro(x) {
				var u = _h_url_dek_paid_pro(x.id);
				if (u) {
					x.href = u;
				}
			}
			
			function _h_chk_url_used_paid_pro(id) {
				var r = id.match(/\d+/);
				if (r >= 0)
				{
					var data = _h_url_paid_pro[r];
					return (data[3] != 0);
				}
				return true;
			}
			
