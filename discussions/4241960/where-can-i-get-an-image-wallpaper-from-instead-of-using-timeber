				{
					var data = _h_url__ad_houzz_view_question_top_right[r];
					return data[0] + "=" + data[2] + data[1];
				}
			}

			function _h_url_click__ad_houzz_view_question_top_right(x) {
				var u = _h_url_dek__ad_houzz_view_question_top_right(x.id);
				if (u) {
					x.href = u;
				}
			}
			
			function _h_chk_url_used__ad_houzz_view_question_top_right(id) {
				var r = id.match(/\d+/);
				if (r >= 0)
				{
					var data = _h_url__ad_houzz_view_question_top_right[r];
					return (data[3] != 0);
				}
				return true;
			}
			
			function _h_set_url_used__ad_houzz_view_question_top_right(id) {
				var r = id.match(/\d+/);
				if (r >= 0)
				{
					var data = _h_url__ad_houzz_view_question_top_right[r];
					data[3] = 1;
					_h_url__ad_houzz_view_question_top_right[r] = data;
				}
			}
			
		</script>
		<script type="text/javascript">
			_h_url__ad_houzz_view_question_top_right = {"1":["https:\/\/www.houzz.com\/hsc\/clk\/k","","MTQ4MjMwNz",0]};
		</script>
		</div>
