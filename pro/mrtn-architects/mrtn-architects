	// <script>
(function() {
	if (window.hzmr)
	{
		window.hzmr.version = "2468";
		window.hzmr.allResources = ["jquery-1.11.1","hz","hzConfig","hzI18n","hzLogger","hzMain","eventBus","main","errSt","jquery-migrate-1.2.1","jqUtils","core","ui","responsiveReload","autoSuggest","standardHeaderV6","CartActionsRequest","SetVisitorPropertyRequest","HoverCardRequest","FollowRequest","LikeRequest","CommentManagerAjaxRequest","FeedAjaxRequest","ContentFlagAjaxRequest","GetMinimalSpaceDataRequest","ClippingRequest","SendEmailRequest","CurrentSessionUserInfoRequest","SearchSuggestionRequest","VerificationEmailRequest","NoOpRequest","AuthorizeRequest","TsdbLoggerAjaxRequest","JsErrorLoggerAjaxRequest","SearchKeywordRequest","GetSpaceAjaxRequest","socialSdk","siteAuth","filterRenderer","hzGtm","standardFooter","scrollToTop","cartActions","signupDialog","newPostSignup","hoverCard","followButton","fontSmoothing","like","jquery-ui-1.10.3.custom.widget-only","flagContent","feedComments","jquery.form.min","spacePreview","zoomable","clipping","profileUtils","contactDialog","edgeContactDialog","simpleCarousel","viewProfileCarousel","invite","multipleLocationsDialog"];
		for (var i=0; i<window.hzmr.length; i++) {
			var filename = window.hzmr[i];
			var v = filename.split(':')[1];
			if (v != window.hzmr.version) {
				HZ.utils.Logger.sendError({msg:"Page version "+window.hzmr.version+" doesn't match file version "+filename, file:null, line:null, url:window.location.href});
				break;
			}
		}
		var missingFiles = [];
		var loadedResources = {};
		for (var i=0; i<hzmr.length; i++) { loadedResources[hzmr[i].match(/(.*):\d*/)[1]] = true; }
		for (var i=0; i<hzmr.allResources.length; i++) {
			key = hzmr.allResources[i];
			if (!loadedResources[key]) {
				missingFiles.push (key);
			}
		}
		if (missingFiles.length > 0) {
			var msg = "JS resource failed to load: " + missingFiles[0] + ".";
			if (missingFiles.length > 1) {
				msg += " Missing out on: " + missingFiles.slice(1).join(",") + ".";
			}
			HZ.utils.Logger.sendError({msg:msg, file:null, line:null, url:window.location.href});
		}
	}
})();
</script>
