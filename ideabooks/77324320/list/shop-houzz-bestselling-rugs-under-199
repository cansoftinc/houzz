<!--			-->			<script>
				(function () {

					var prevSelectedLanguages = ["en"],
						languageCountMap = [];

					function setCommentCountLabel (selectedLanguages, hasLanguagesChanged) {
						var commentCount = 0,
							$commentCount = $('.viewGalleryComments .comment-count-label');
						for (var i = 0; i < selectedLanguages.length; i++) {
							var language = selectedLanguages[i];
							if (languageCountMap.hasOwnProperty(language)) { // TODO: languageCountMap may be inaccurate
								commentCount += parseInt(languageCountMap[language]);
							}
						}
						if ($commentCount.length && hasLanguagesChanged) {
							var text = 'Comments';
							if (commentCount) {
								text += '  (' + commentCount + ')';
							}
							$commentCount.text(text);
						}
						return commentCount;
					}

					function setMoreCommentsButton (selectedLanguages, commentCount) {
						var $showMoreComments = $('.moreComments');
						if ($showMoreComments.length) {
							var displayedCommentsCount = 0;
							for (var i = 0; i < selectedLanguages.length; i++) {
								var comments = $('.commentsContainer').children('.comment[data-hz*=' + selectedLanguages[i] + ']');
								displayedCommentsCount += comments.length;
							}
							var remainingCommentsCount = commentCount - displayedCommentsCount;
							if (remainingCommentsCount > 0) {
								$showMoreComments.find('.remainingCount').text(remainingCommentsCount);
							}
							$showMoreComments.toggle(remainingCommentsCount > 0);
						}
					}

					function getHasLanguagesChanged (selectedLanguages) {
						var hasLanguagesChanged = false;
						if (selectedLanguages.length == prevSelectedLanguages.length) {
							for (var i = 0; i < selectedLanguages.length; i++) {
								if ($.inArray(selectedLanguages[i], prevSelectedLanguages) == -1) {
									hasLanguagesChanged = true;
									prevSelectedLanguages = selectedLanguages;
									break;
								}
							}
						} else {
							hasLanguagesChanged = true;
							prevSelectedLanguages = selectedLanguages;
						}
						return hasLanguagesChanged;
					}

					HZ.globalComments.init({
						container: $('.viewGalleryComments')[0],
						globalCommentsEnabled: false,
						galleryId: "77324320",
						languageCountMap: [],
						commentIdsToComments: [],
						languagesToCommentIds: [],
						allLanguages: {"en":{"text":"English","enabled":false},"de":{"text":"German","enabled":false},"fr":{"text":"French","enabled":false},"ru":{"text":"Russian","enabled":false},"ja":{"text":"Japanese","enabled":false},"it":{"text":"Italian","enabled":false},"es":{"text":"Spanish","enabled":false},"da":{"text":"Danish","enabled":false},"nb":{"text":"Norwegian","enabled":false},"fi":{"text":"Finnish","enabled":false},"sv":{"text":"Swedish","enabled":false},"nl":{"text":"Dutch","enabled":false},"other":{"text":"Other","enabled":false}},
						selectedLanguages: ["en"],
						defaultSiteLanguage: 'en',
						onSelectLanguages: function (selectedLanguages) {
							var hasLanguagesChanged = getHasLanguagesChanged(selectedLanguages);
							var commentCount = setCommentCountLabel(selectedLanguages, hasLanguagesChanged);
							setMoreCommentsButton(selectedLanguages, commentCount);
						}
					});

				})();
			</script>
			


